{
  "driver": "postgresql",
  "name": "cloak_postgres_native",
  "parameters": {
    "hostname": "localhost",
    "username": "cloak",
    "database": "cloak"
  },
  "tables": {
    "test": {
      "db_name": "user_data",
      "user_id": "user_id"
    },
    "purchases": {
      "db_name": "purchases",
      "user_id": "uid"
    },
    "demographics": {
      "db_name": "demographics",
      "user_id": "uid"
    },
    "demographics_as_query": {
      "query": "
        SELECT *
        FROM demographics
      ",
      "user_id": "uid"
    },
    "customers": {
      "db_name": "customers",
      "keys": [
        {"user_id": "id"}
      ]
    },
    "accounts": {
      "db_name": "accounts",
      "auto_isolating_column_classification": false,
      "isolating_columns": {"swift": false, "account_number": true},
      "keys": [
        {"user_id": "customer_id"},
        {"account": "id"}
      ]
    },
    "transactions": {
      "db_name": "transactions",
      "keys": [
        {"account": "account_id"}
      ]
    },
    "integers": {
      "db_name": "integers",
      "user_id": "user_id"
    }
  }
}
