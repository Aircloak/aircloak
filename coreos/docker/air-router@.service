[Unit]
Description=Air router
After=etcd2.service
After=docker.service
After=air-keys.service
Requires=etcd2.service
Requires=docker.service

[Service]
TimeoutStartSec=0
ExecStart=/aircloak/air/air_service_ctl.sh start_service router
ExecStop=/aircloak/air/air_service_ctl.sh stop_service router

[X-Fleet]
Conflicts=air-router@*.service
