  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "staticFiles": {
    "staticPath": "static"
  },
  "browserslist": ">5%, firefox >= 60, chrome >= 72, last 1 safari version, last 1 edge version"
