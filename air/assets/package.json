  "scripts": {
    "test": "mocha --reporter dot --require @babel/register test/**/*.js",
    "deploy": "parcel build js/{app,docs}.js --out-dir ../priv/static/frontend --public-url /frontend/ --no-autoinstall",
    "watch": "PARCEL_AUTOINSTALL=false parcel watch js/{app,docs}.js --out-dir ../priv/static/frontend --public-url /frontend/",
    "build-dev": "NODE_ENV=development parcel build js/{app,docs}.js --no-minify --out-dir ../priv/static/frontend --public-url /frontend/  --no-autoinstall",
    "lint": "eslint --max-warnings 0 js test && prettier --version && prettier --no-config -c css/**.css \"../*.md\" \"../priv/**/*.md\"",
