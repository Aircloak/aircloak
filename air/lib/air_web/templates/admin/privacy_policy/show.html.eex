<ol class="breadcrumb">
  <li class="breadcrumb-item"><%= link "Privacy policy", to: admin_privacy_policy_path(@conn, :index) %></li>
  <li class="breadcrumb-item active">Revision <%= @privacy_policy.id %></li>
</ol>

<h2>Privacy policy revision <%= @privacy_policy.id %></h2>

<p>This version was created on the <%= format_datetime(@privacy_policy.inserted_at) %>.</p>

<%= if AirWeb.PrivacyPolicyView.has_change_info?(@privacy_policy) do %>
  <h3>Changes in this revision</h3>
  <%= raw(AirWeb.PrivacyPolicyView.changes_as_html(@privacy_policy)) %>
<% end %>

<h3>Privacy policy content</h3>
<div id="privacy-policy">
  <%= raw(AirWeb.PrivacyPolicyView.content_as_html(@privacy_policy)) %>
</div>
