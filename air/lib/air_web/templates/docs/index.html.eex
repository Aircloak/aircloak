<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Aircloak â€“ User Guide</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="Aircloak User Guide" />
    <meta
      name="viewport"
      content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"
    />
    <link rel="shortcut icon" href="<%= static_path(@conn, "/frontend/images/favicon.ico") %>" type="image/x-icon">
    <link rel="stylesheet" href="<%= static_path(@conn, "/frontend/app.css") %>" />
    <link rel="stylesheet" href="<%= static_path(@conn, "/docs/assets/css/vue.css") %>" />
  </head>
  <body>
    <%= render AirWeb.LayoutView, "_navbar.html", conn: @conn, admin: false %>
    <div class="docsify">
      <div id="app"></div>
    </div>
    <script>
      window.$docsify = {
        // Branding
        themeColor: "#1EB9D6",

        // Routing
        homepage: "introduction.md",
        basePath: "/docs",
        auto2top: true,

        // Search
        search: {
          paths: "auto",
        },

        // Sidebar
        loadSidebar: true,
        alias: {
          "/.*/_sidebar.md": "/_sidebar.md"
        },
        subMaxLevel: 2,
        topMargin: 60,
        plugins: [
          function(hook) {
            var footer = <%= to_json(render_to_string AirWeb.SharedView, "footer.html", conn: @conn) %>;
            hook.afterEach(function (html) { return html + footer });
          }
        ]
      };
    </script>
    <script src="<%= static_path(@conn, "/frontend/app.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/docsify.min.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/search.min.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/docsify-tabs.min.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/prism-sql.min.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/prism-json.min.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/prism-bash.min.js") %>"></script>
    <script src="<%= static_path(@conn, "/docs/assets/js/prism-ruby.min.js") %>"></script>
  </body>
</html>
