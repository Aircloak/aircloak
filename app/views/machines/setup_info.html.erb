<% if @cloak.cluster -%>
build_url="http://build.aircloak.com/releases/<%=@cloak.cluster.build.id%>.tar.gz"
base_image_tag="<%=@cloak.cluster.os_tag.name%>"
perform_aircloak_install=true
root_certificates="<%=@cloak.cluster.get_root_certificates%>"
<% else -%>
base_image_tag="<%=OsTag.last.name%>"
perform_aircloak_install=false
root_certificates="<%=Cluster.get_root_certificates%>"
<% end -%>
